body{
	margin:0;
	padding:0;
	font: 14px/1.5 Tahoma,Helvetica,Arial,'宋体',sans-serif;
}
a{
    text-decoration:none;
    color:#000;	
}
.text{
	border:1px solid #ccc;
	padding:5px;
	border-radius:5px;
	transition:all 0.3s;
}
.text:focus{
	box-shadow:0 0 5px 3px #ccc;
	border:1px solid #000;
}
.container{
    margin:0 auto;
    padding:0;
    width:100%;
}
.panel{
    margin:0;
    padding:0;
}
.panel .panel-search{
	padding:10px;
	background:#eee;
	border:1px solid #ccc;
}
.button{
	border:none;
	background-color:#fff;
	padding:5px 10px 5px 10px;
	font-size:14px;
	cursor:pointer;
}
.button.button-default{
	border:1px solid #ccc;
	border-radius:10px;
	background-color:#F3F3F3;
}
.button.button-default:hover{
	background-color:#E6E6E6;
}
.button.button-default:active{
	background-color:#ccc;
	padding:5px 9px 5px 11px;
}
.image{
	height:16px;
	line-height:16px;
	padding:5px;
	border:1px solid #eee !important;
	margin-left:10px;
}
.image:hover{
	border:1px solid #666 !important;
	box-shadow:0 0 3px #000;
}
.panel .panel-title{
	font-weight:bold;
	text-align:center;
	letter-spacing:5px;
	font-family:黑体;
	font-size:20px;
	padding:10px;
	text-shadow:2px 2px 7px #000;
	height:50px;
}
.panel .panel-title h3{
	font-size:25px;
	padding:0;
	margin:0;
	height:50px;
	line-height:50px;
	display:inline;
}
.panel .panel-body{
	
}
.panel .panel-pager{
	text-align:right;
	margin-top:10px;
}
.panel .panel-pager .pager{
	float:right;
}
.panel .panel-pager a,span{
	float:left;
	border:1px solid #ccc;
	padding:6px;
	margin:0 !important;
	width:20px;
	height:20px;
	font-size:12px;
	line-height:20px;
	text-align:center;
	font-weight:bold;
	white-space:nowrap;
}
.panel .panel-pager a:first-child{
    border-radius:5px 0 0 5px;
}
.panel .panel-pager a:last-child{
    border-radius:0 5px 5px 0;
}
.panel .panel-pager span{
	background-color:#888;
	color:#fff !important;
	border-color:#888;
}
.panel .panel-pager a[href]:hover{
	background-color:#888;
	border-color:#888;
	color:#fff;
}
.table{
	width:100%;
	border:none;
}
.table-border{
	border:1px solid #ccc;
}
.table th{
	padding:5px 5px 10px 5px;
	border:none;
	border-bottom:1px solid #777;
}
.table td{
	padding:5px 3px;
	text-align:center;
	border:none;
	border-bottom:1px solid #ccc;
}
.table tr:nth-child(1){
	margin-bottom:3px;
}
.text-left{
	text-align:left;
}
.table tr{
    transition:all 0.2s;
} 
.table tr:nth-child(2n){
	background-color:#eee;
} 
.table tr:not(:first-child):hover{
	cursor:pointer;
	background-color:#A74A4A;
	color:#fff;
}