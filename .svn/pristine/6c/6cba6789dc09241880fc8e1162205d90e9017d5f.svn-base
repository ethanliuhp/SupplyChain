using System;
using System.Text;
using System.Runtime.Serialization;

namespace Application.Business.Erp.PortalIntegration.Domain
{
    /// <summary>
    /// 工程项目信息
    /// </summary>
    [DataContract]
    public class ProjectInfo
    {

        /// <summary>
        /// ID
        /// </summary>
        [DataMember]
        public string Id;
        
        /// <summary>
        /// 项目代码
        /// </summary>
        [DataMember]
        public string Code;

        /// <summary>
        /// 项目名称
        /// </summary>
        [DataMember]
        public string Name;

        /// <summary>
        /// 承包范围
        /// </summary>
        [DataMember]
        public string ContractArea;

        /// <summary>
        /// 创建时间
        /// </summary>
        [DataMember]
        public DateTime CreateDate;

        /// <summary>
        /// 工程地点 说明
        /// </summary>
        [DataMember]
        public string ProjectLocationDescript;

        /// <summary>
        /// 工程地点 市
        /// </summary>
        [DataMember]
        public string ProjectLocationCity;

        /// <summary>
        /// 工程地点 省
        /// </summary>
        [DataMember]
        public string ProjectLocationProvince;

        /// <summary>
        /// 地下层数
        /// </summary>
        [DataMember]
        public decimal UnderGroundLayers;

        /// <summary>
        /// 地上层数
        /// </summary>
        [DataMember]
        public decimal GroundLayers;

        /// <summary>
        /// 建筑面积
        /// </summary>
        [DataMember]
        public decimal BuildingArea;

        /// <summary>
        /// 建筑高度
        /// </summary>
        [DataMember]
        public decimal BuildingHeight;

        /// <summary>
        /// 立项责任人名称
        /// </summary>
        [DataMember]
        public string HandlePersonName;

        /// <summary>
        /// 所属项目部 组织层次码
        /// </summary>
        [DataMember]
        public string OwnerOrgSysCode;

        /// <summary>
        /// 所属项目部
        /// </summary>
        [DataMember]
        public string OwnerOrg;

        /// <summary>
        /// 所属项目部名称(业务组织名称)
        /// </summary>
        [DataMember]
        public string OwnerOrgName;
        /// <summary>
        /// 项目经理部
        /// </summary>
        [DataMember]
        public string ManagerDepart;

        /// <summary>
        /// 质量目标
        /// </summary>
        [DataMember]
        public string QuanlityTarget;

        /// <summary>
        /// 安全目标
        /// </summary>
        [DataMember]
        public string SaftyTarget;

        /// <summary>
        /// 进场日期
        /// </summary>
        [DataMember]
        public DateTime AprroachDate;

         /// <summary>
        /// 开工日期
        /// </summary>
        [DataMember]
        public DateTime BeginDate;

        /// <summary>
        /// 竣工日期
        /// </summary>
        [DataMember]
        public DateTime EndDate;

         /// <summary>
        /// 工程造价（万元）
        /// </summary>
        [DataMember]
        public decimal ProjectCost;

         /// <summary>
        /// 项目生命周期(施工阶段)
        /// </summary>
        [DataMember]
        public string ProjectLifeCycle;
    }
}
