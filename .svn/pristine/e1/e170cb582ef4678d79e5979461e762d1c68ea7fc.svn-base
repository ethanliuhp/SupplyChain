using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Application.Business.Erp.SupplyChain.Base.Domain;

namespace Application.Business.Erp.SupplyChain.FinanceMng.Domain
{
    /// <summary>
    /// 个人划账单
    /// </summary>
    [Serializable]
    public class DelimitIndividualBill : BaseMaster
    {
        private DateTime repayDate;
        private string borrower;
        private DateTime borrowerDate;
        private string borrowerUse;
        private string nodeNumber;
        private string payment;
        private Double totalMoney;

        /// <summary>
        /// 还款日期
        /// </summary>
        virtual public DateTime RepayDate
        {
            get { return repayDate; }
            set { repayDate = value; }
        }
        /// <summary>
        /// 借款人
        /// </summary>
        virtual public string Borrower
        {
            get { return borrower; }
            set { borrower = value; }
        }
        /// <summary>
        /// 借款用途
        /// </summary>
        virtual public string BorrowerUse
        {
            get { return borrowerUse; }
            set { borrowerUse = value; }
        }
        /// <summary>
        /// 借款日期
        /// </summary>
        virtual public DateTime BorrowerDate
        {
            get { return borrowerDate; }
            set { borrowerDate = value; }
        }
        /// <summary>
        /// 票据号码
        /// </summary>
        virtual public string NodeNumber
        {
            get { return nodeNumber; }
            set { nodeNumber = value; }
        }
        /// <summary>
        /// 支付方式
        /// </summary>
        virtual public string Payment
        {
            get { return payment; }
            set { payment = value; }
        }
        /// <summary>
        /// 总金额
        /// </summary>
        virtual public Double TotalMoney
        {
            get { return totalMoney; }
            set { totalMoney = value; }
        }
    }
}
