using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Application.Business.Erp.SupplyChain.Base.Domain;

namespace Application.Business.Erp.SupplyChain.ConcreteManage.PouringPlanMng.Domain
{
    /// <summary>
    /// 浇筑计划明细
    /// </summary>
    [Serializable]
    public class PouringPlanDetail : BaseDetail
    {
        private decimal leftQuantity;
        private string slump;

        /// <summary>
        /// 塌落度
        /// </summary>
        virtual public string Slump
        {
            get { return slump; }
            set { slump = value; }
        }
        /// <summary>
        /// 剩余数量
        /// </summary>
        virtual public decimal LeftQuantity
        {
            get { return leftQuantity; }
            set { leftQuantity = value; }
        }
    }
}
