/*-site toolbar style,站点工具条用到的所有样式-*/

.ToolBarFoot
{
    /* background-image: url("images/ToolBar/separate.png");	  */
    border: 0px solid #000;
    border-top: none;
    border-bottom: none;
    background-color: #DEEFF9;
}

.ToolBar
{
    padding-left: 1px;
    padding-right: 1px;
    padding-top: 6px;
    padding-bottom: 6px;
}

.TBItem
{
    /*background-color: #336699;	 */
    border: solid 0px #336699;
    font-family: Arial;
    font-size: 9pt;
    color: #1D73CB;
    cursor: pointer; /*background-image: url("images/ToolBar/toolbarBg.gif");   */
}

.TBItemHover
{
    background-color: #f1f8ff;
    border: solid 0px #336699;
    border-left-color: #EEEEEE;
    border-top-color: #EEEEEE;
    border-bottom-color: #999999;
    border-right-color: #999999;
    font-family: Arial;
    font-size: 9pt;
    cursor: pointer;
}

.TBItemActive
{
    background-color: #b9d1ea;
    border: solid 0px #b9d1ea; /*#f1f8ff*/
    border-left-color: #999999;
    border-top-color: #999999;
    border-bottom-color: #EEEEEE;
    border-right-color: #EEEEEE;
    font-family: Arial;
    font-size: 9pt;
    cursor: pointer;
}

/*site tabstrip style, 站点标签页用到的所有样式 */
.TopTabGroup
{
    z-index: 99;
    position: relative;
    clear: both; /*background-color: #deeff9;*/
}

.DefaultTab
{
    color: #064687;
    background-image: url(../images/tabstrip/tabs_hover_center.png); /*background-image: url(../images/tabstrip/tab_middle.gif);*/
    font-family: Arial;
    font-size: 12px;
    cursor: pointer;
    border-left: 1px solid #B0D7FF;
    border-right: 1px solid #B0D7FF;
    border-top: 0px solid #B0D7FF;
    border-bottom: 1px solid #B0D7FF;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.DefaultTabHover
{
    color: #064687;
    background-image: url(../images/tabstrip/tabs_hover_center.png);
    font-family: Arial;
    font-size: 12px;
    cursor: pointer;
    border-left: 1px solid #B0D7FF;
    border-right: 1px solid #B0D7FF;
    border-top: 0px solid #B0D7FF;
    border-bottom: 1px solid #B0D7FF;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.SelectedTab
{
    background-image: url(../images/tabstrip/tab_selected_middle.gif);
    font-family: Arial;
    font-size: 12px;
    cursor: default;
    color: #00368F;
}

.MultiPage
{
    border-style: solid;
    border-width: 0px 0px 0px 0px;
