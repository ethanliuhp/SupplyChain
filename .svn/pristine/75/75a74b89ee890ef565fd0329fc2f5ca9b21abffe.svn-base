.dialog{
    top:100px;
    left:200px;
    position: absolute;
    padding:20px;
    display: none;
    width:400px;
    z-index: 1001;
    background-color: #fff;
    height:auto;
    border:1px solid #777;
    border-radius: 5px;
    box-shadow: 1px 1px 10px 0px #000
}
.dialog table{
}
.page-shadow{
    top:0;
    left:0;
    position: absolute;
    width:100%;
    background-color:#000;
    opacity: 0.3;
    filter:alpha(opacity=30);
    z-index: 1000;
}