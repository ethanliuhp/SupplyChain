using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using Application.Business.Erp.SupplyChain.Base.Domain;

namespace Application.Business.Erp.SupplyChain.EngineerManage.MeetingManage.Domain
{
    [Serializable]
    public class MeetingMng:BaseMaster
{
        private string attemdee;                       //参会人员
        private string meetingAdd;                     //会议地点
        private string meetingStyle;                     //会议类型
        private DateTime meetingDate;                 //会议日期
        private string meetingRemark;                    //会议说明
        private string meetingTopic;                       //会议主题
        /// <summary>
        /// 参会人员
        /// </summary>
        virtual public string Attemdee
        {
            get { return attemdee; }
            set { attemdee = value; }
        }
   
       /// <summary>
        /// 会议地点
       /// </summary>
        virtual public string MeetingAdd
        {
            get { return meetingAdd; }
            set { meetingAdd = value; }
        }
      
        /// <summary>
        /// 会议类型
        /// </summary>
        virtual public string MeetingStyle
        {
            get { return meetingStyle; }
            set { meetingStyle = value; }
        }

        /// <summary>
        /// 会议日期
        /// </summary>
        virtual public DateTime MeetingDate
        {
            get { return meetingDate; }
            set { meetingDate = value; }
        }

        /// <summary>
        /// 会议说明
        /// </summary>
        virtual public string MeetingRemark
        {
            get { return meetingRemark; }
            set { meetingRemark = value; }
        }
   
        /// <summary>
        /// 会议主题
        /// </summary>
        virtual public string MeetingTopic
        {
            get { return meetingTopic; }
            set { meetingTopic = value; }
        }
    }
}
