      <generator class="VirtualMachine.Component.Util.IFCGuidGenerator,VirtualMachine" />
    </id>
    <version name="Version" column="VERSION" type="long" unsaved-value="-1"/>

    <property name="NGUID"/>
    <!--标前成本工程量-->
    <property name="QDProjectCost"/>
    <!--标前成本综合单价-->
    <property name="QDPrice"/>
