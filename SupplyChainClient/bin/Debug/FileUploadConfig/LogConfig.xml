	<!--
	PLMLOGDIR  log file directory.
	           You can define it as environment variable
	           otherwise the PLMPLMLoggerData folder will be
	           created under file system root.
	-->
    <appender name="StackTrace" type="log4net.Appender.RollingFileAppender">
        <file type="log4net.Util.PatternString" value="%property{PLMLOGDIR}/PLMLoggerData/StackTrace.log" />
        <staticLogFileName value="true" />
        <appendToFile value="true" />
        <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
        <maximumFileSize value="10000KB" />
        <maxSizeRollBackups value="30" />
        <rollingStyle value="Date" />
        <datePattern value="yyyyMMdd" />
        <layout type="log4net.Layout.XmlLayoutSchemaLog4j">
            <locationInfo value="false" />
        </layout>
    </appender>
